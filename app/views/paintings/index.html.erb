<p id="notice"><%= notice %></p>
<h1>Malarstwo</h1>

<ul>
  <li><%= link_to 'Dodaj Obrazek', new_painting_path %></li>
  <li><%= link_to 'Powrót', static_pages_upload_path %></li>
</ul>



  <% @paintings.each do |painting| %>
  <div class="paintings-index">
  <%= link_to image_tag("#{painting.my_file.url(:thumb)}"), painting %>
     <div class="paintings-links">
       <a href="<%= painting.my_file %>">Pobierz</a>
       <%= link_to 'Edycja', edit_painting_path(painting) %>
       <%= link_to 'Usuń', painting, method: :delete, data: { confirm: 'Are you sure?' } %>
    </div>
</div>
<% end %>



